{
  "name": "team-city-build-screen",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "testNoWatch": "npx jest --verbose",
    "test": "npx jest --watch",
    "netlifybuild": "npm run lint && npm run testNoWatch && npm run productionBuild",
    "productionBuild": "ng build --prod && node buildscript/write-config.js && sh buildscript/associate-sentry-release-and-commit.sh",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update @angular/cli @angular/core"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- TeamCityBuildScreen"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@sentry/browser": "5.24.2",
    "core-js": "^3.2.1",
    "date-fns": "^2.16.1",
    "ngx-clipboard": "^13.0.1",
    "ngx-date-fns": "^6.5.0",
    "rxjs": "~6.5.4",
    "subsink": "^1.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/language-service": "~11.0.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "2.0.3",
    "@types/jest": "^26.0.15",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.2",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.1",
    "jest-preset-angular": "^8.3.2",
    "protractor": "~7.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
